{% extends 'base.html.twig' %}

{% block title %}Tipas index{% endblock %}

{% block body %}
    <h1>Tipas index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Pavadinimas</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tipa in tipas %}
            <tr>
                <td>{{ tipa.id }}</td>
                <td>{{ tipa.pavadinimas }}</td>
                <td>
                    <a href="{{ path('tipas_show', {'id': tipa.id}) }}">show</a>
                    <a href="{{ path('tipas_edit', {'id': tipa.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tipas_new') }}">Create new</a>
{% endblock %}
